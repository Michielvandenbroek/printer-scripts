*Feature: PaperSize
{
	*rcNameID: =IDS_SCALING_PAPERSIZE
	*Option: LETTER
	{
		*rcNameID: =RCID_DMPAPER_SYSTEM_NAME
		*PrintSchemaKeywordMap: "NorthAmericaLetter"
		*Switch: JobDeviceMarigin
		{
			*Case: Margin0
			{
				*PrintableArea: PAIR(10200, 13200)
				*PrintableOrigin: PAIR(0, 0)
				*Switch: Orientation
				{
					*Case: PORTRAIT
					{
						*CursorOrigin: PAIR(0, 0)
					}
					*Case: LANDSCAPE_CC90
					{
						*CursorOrigin: PAIR(0, 13200)
					}
				}
			}
		   *Case: Margin70
		   {
				*PrintableArea: PAIR(9920, 12920)
				*PrintableOrigin: PAIR(140, 140)
				*Switch: Orientation
				{
					*Case: PORTRAIT
					{
						*CursorOrigin: PAIR(140, 140)
					}
					*Case: LANDSCAPE_CC90
					{
						*CursorOrigin: PAIR(140, 13060)
					}
				}
		   }
		   *Case: Margin100
		   {
				*PrintableArea: PAIR(9800, 12800)
				*PrintableOrigin: PAIR(200, 200)
				*Switch: Orientation
				{
					*Case: PORTRAIT
					{
						*CursorOrigin: PAIR(200, 200)
					}
					*Case: LANDSCAPE_CC90
					{
						*CursorOrigin: PAIR(200, 13000)
					}
				}
		   }
		   *Case: Margin5mm 
		   {
				*PrintableArea: PAIR(9728, 12728)
				*PrintableOrigin: PAIR(236, 236)
				*Switch: Orientation
				{
					*Case: PORTRAIT
					{
						*CursorOrigin: PAIR(236, 236)
					}
					*Case: LANDSCAPE_CC90
					{
						*CursorOrigin: PAIR(236, 12964)
					}
				}				
		   }
		   *Case: MarginEdgeToEdge
		   {
				*PrintableArea: PAIR(10008, 13008)
				*PrintableOrigin: PAIR(96, 96)
		   }
		   *default:
		   {
				*PrintableArea: PAIR(9728, 12728)
				*PrintableOrigin: PAIR(236, 236)
				*Switch: Orientation
